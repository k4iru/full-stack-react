    },
    "homepage": "https://github.com/k4iru/full-stack-react#readme",
    "dependencies": {
        "babel-polyfill": "^6.26.0",
        "express": "^4.17.1",
        "minimist": "^1.2.3",
        "react": "^16.13.0",
        "react-dom": "^16.13.0"
    },
    "devDependencies": {
        "@babel/core": "^7.8.7",
        "@babel/plugin-proposal-class-properties": "^7.8.3",
        "@babel/preset-env": "^7.8.7",
        "@babel/preset-react": "^7.8.3",
        "acorn": "^7.1.1",
        "babel-eslint": "^10.1.0",
